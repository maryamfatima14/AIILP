{
  "name": "aiilp-root",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=aiilp-backend\" \"npm run dev --workspace=aiilp-frontend\"",
    "build": "npm run build --workspace=aiilp-frontend",
    "vercel-build": "npm run build --workspace=aiilp-frontend",
    "install-all": "npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "vite": "^4.5.0"
  }
}